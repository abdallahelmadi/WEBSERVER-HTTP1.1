{
  "servers": [
    {
      "port": "3000",
      "name": "my-app",
      "version": "0.1.0",
      "index": "index.html",
      "root": "./",
      "routes": [

        { "path": "/upload", "source": "upload.html" },

        { "path": "/list", "dictlist": "true" },
        { "path": "/list/home", "dictlist": "true" },
        { "path": "/list/home/test", "dictlist": "true" },
        { "path": "/list/folder", "dictlist": "true" },
        { "path": "/list/hello", "dictlist": "true" },

        { "path": "/redirect1", "redirect": "/redirect2" },
        { "path": "/redirect2", "redirect": "https://www.youtube.com/watch?v=yc7x5jNhXIQ" },

        {
          "path": "/cgi",
          "cgi_script": "python.py",
          "cgi_interpreter": "/usr/bin/python3"
        }

      ]
    }
  ]
}